pipeline {
    agent any
    
        stages {
            stage ('verify git branch') {
                steps {
                    echo "$GIT_BRANCH"
                }
            }
            stage ('verify git branch') {
                steps {
                    sh 'ls'
                }
            }
            stage ('verify git branch') {
                steps {
                    echo "the stage is complete"
                }
            }
        }
    }